<%- include('partials/header', { title: "Admin Dashboard", user: user || null }) %>

<body class="admin-page"> <!-- Add the class 'admin-page' here -->

<h1>Admin Dashboard</h1>
<p>Welcome, <%= user.username %>! You have admin privileges.</p>

<div class="user-list">
    <h2>Users List</h2>
    <ul>
      <% users.forEach(u => { %>
        <li><%= u.username %> - <%= u.role %></li>
      <% }) %>
    </ul>
  </div>  

<%- include('partials/footer') %>
